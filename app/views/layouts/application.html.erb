<!DOCTYPE html>
<html>
  <head>
    <title>Cbuacm</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <link rel="stylesheet" href="https://bootswatch.com/4/darkly/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha256-4+XzXVhsDmqanXGHaHvgh1gMQKX40OUvDEBTu8JcmNs=" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/@popperjs/core@2"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
  </head>

  <body class="container-fluid light-gray">
    <div id="header">
      <div class="row blue-faded" style="height: 45px;"></div>

      <div id="menu-container" class="row gold" style="height: 30px; overflow: visible;">
        <div id="menu-box" class="row col-sm maj-head-bar banner-width">
          <div id="logo-container" class="col-6">
            <div class="maj-head-box">
              <div class="maj-head-dash row">
                <div id="left-logo" class="col-sm">
                  <h2>ACM</h2>
                </div>

                <div id="right-logo" class="col-sm">
                  <h2>CBU</h2>
                </div>
              </div>
            </div>
          </div>

          <div class="menu-button col-3 center">
            <h3 class="center">Home</h3>
          </div>

          <div class="menu-button col-3 center">
            <h3 class="center">Hall of Fame</h3>
          </div>
        </div>
      </div>
    </div>

    <div id="front-img" class="row">
      <img src="IMG_000.jpg" class="content-width"/>
    </div>

    <div id="about-us" class="row">
      
    </div>

    <div id="links-connections" class="row">
      
    </div>

    <div id="projects" class="row">
      
    </div>

    <div id="member-rotation" class="row">
      
    </div>

    <div id="footer" class="row">
      
    </div>
  </body>
</html>
